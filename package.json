{
  "name": "@riveo/payload-starter",
  "version": "1.0.0",
  "description": "Payload 3.0 starter",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "format": "cross-env NODE_OPTIONS=--no-deprecation prettier \"**/*.{cjs,js,ts,tsx,json}\" --write",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@payload-enchants/cached-local-api": "^1.2.2",
    "@payloadcms/db-mongodb": "3.0.0-beta.120",
    "@payloadcms/next": "3.0.0-beta.120",
    "@payloadcms/plugin-nested-docs": "3.0.0-beta.120",
    "@payloadcms/richtext-lexical": "3.0.0-beta.120",
    "@payloadcms/storage-s3": "3.0.0-beta.120",
    "@payloadcms/ui": "3.0.0-beta.120",
    "@radix-ui/react-slot": "^1.1.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "highlight.js": "^11.10.0",
    "lodash": "^4.17.21",
    "next": "^15.0.0",
    "payload": "3.0.0-beta.120",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "sharp": "0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.0-canary.173",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^20.15"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
